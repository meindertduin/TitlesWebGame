@page "/game"
@inject GameSessionState GameSessionState;
@using TitlesWebGame.WebUi.Services
@implements IDisposable;

<GameSessionLobby /> 

@code {
    
    protected override void OnInitialized()
    {
        GameSessionState.OnChange += StateHasChanged;
    }

    public void Dispose()
    {
        GameSessionState.OnChange -= StateHasChanged;
    }

}